<plugin name='rapid-grails' version='0.1' grailsVersion='2.2.3 &gt; *'>
  <author>Farshid Zaker</author>
  <title>Rapid Grails Plugin</title>
  <description>Brief summary/description of the plugin.
</description>
  <documentation>http://grails.org/plugin/rapid-grails</documentation>
  <type>RapidGrailsGrailsPlugin</type>
  <resources>
    <resource>rapidgrails.RapidGrailsController</resource>
    <resource>rapidgrails.RapidGrailsService</resource>
    <resource>rapidgrails.ReportService</resource>
    <resource>rapidgrails.CommonTagLib</resource>
    <resource>rapidgrails.FormTagLib</resource>
    <resource>rapidgrails.HighchartsTagLib</resource>
    <resource>rapidgrails.JqgridTagLib</resource>
    <resource>rapidgrails.JqueryUiTagLib</resource>
    <resource>rapidgrails.JstreeTagLib</resource>
    <resource>rapidgrails.PopupTagLib</resource>
    <resource>rapidgrails.RapidGrailsTagLib</resource>
    <resource>rapidgrails.SearchTagLib</resource>
    <resource>rapidgrails.search.CriteriaTagLib</resource>
  </resources>
  <repositories>
    <repository name='grailsCentral' url='http://grails.org/plugins' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
    <repository name='http://jasperreports.sourceforge.net/maven2/' url='http://jasperreports.sourceforge.net/maven2/' />
  </repositories>
  <dependencies>
    <compile>
      <dependency group='org.codehaus.gpars' name='gpars' version='1.1.0' />
      <dependency group='org.codehaus.jsr166-mirror' name='jsr166y' version='1.7.0' />
    </compile>
    <runtime>
      <dependency group='mysql' name='mysql-connector-java' version='5.1.22' />
    </runtime>
    <build>
      <dependency group='ar.com.fdvs' name='DynamicJasper' version='3.0.6' />
      <dependency group='net.sf.jasperreports' name='jasperreports' version='5.1.0' />
    </build>
  </dependencies>
  <plugins>
    <compile>
      <plugin group='org.grails.plugins' name='joda-time' version='1.4' />
      <plugin group='org.grails.plugins' name='excel-import' version='1.0.0' />
      <plugin group='org.grails.plugins' name='cache' version='1.0.1' />
    </compile>
    <runtime>
      <plugin group='org.grails.plugins' name='spring-security-core' version='1.2.7.3' />
      <plugin group='org.grails.plugins' name='hibernate' version='2.2.3' />
      <plugin group='org.grails.plugins' name='fields' version='1.3' />
      <plugin group='org.grails.plugins' name='jquery-ui' version='1.8.24' />
      <plugin group='org.grails.plugins' name='jquery' version='1.8.3' />
      <plugin group='org.grails.plugins' name='resources' version='1.2' />
      <plugin group='org.grails.plugins' name='famfamfam' version='1.0.1' />
    </runtime>
    <build>
      <plugin group='org.grails.plugins' name='tomcat' version='2.2.3' />
    </build>
  </plugins>
  <runtimePluginRequirements />
  <behavior />
</plugin>